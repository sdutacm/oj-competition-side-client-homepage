{
  "name": "oj-competition-side-client-homepage",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "SDUT OJ Competition Side Client 官网",
  "author": "sdutacm",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "build:analyze": "vite build --mode analyze",
    "clean": "rimraf dist",
    "build:clean": "npm run clean && npm run build",
    "serve": "npm run build && npm run preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "analyze": "node scripts/analyze-bundle.js",
    "build:report": "npm run build && npm run analyze",
    "build:size": "npm run build && du -sh dist/* | sort -hr",
    "preview:open": "npm run preview && open http://localhost:4173",
    "serve:python": "cd dist && python3 -m http.server 8080",
    "serve:node": "npx serve dist -p 8080"
  },
  "dependencies": {
    "element-plus": "^2.10.4",
    "gsap": "^3.13.0",
    "less": "^4.4.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "compression": "^1.8.1",
    "cross-env": "^10.0.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.27.0",
    "vite": "^7.0.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.7.7"
  }
}
